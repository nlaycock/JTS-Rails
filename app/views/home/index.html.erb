<h1>Home#index</h1>
<p>Find me in app/views/home/index.html.erb</p>

<p>create account here <%=link_to('registration', user_registration_path )%> </p>


<p>user signed in? <%= user_signed_in? %></p>
<% if user_signed_in? %>
<p>Logged in users email: <%= current_user.email %></p>
<p>Logged in users role: <%= current_user.role.name %></p>


<p><a data-method="delete" href="/users/sign_out" rel="nofollow">Sign out</a></p>
<% end %>

<% if not user_signed_in? %>
    <%=link_to('login', new_user_session_path )%>
<% end %>